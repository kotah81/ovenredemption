<Application x:Class="OvenRedemption.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:OvenRedemption"
             StartupUri="MainWindow.xaml"
             
             >
    
    <Application.Resources>
        <FontFamily x:Key="SpaceMono">pack://application:,,,/Fonts/#Space Mono</FontFamily>
        <FontFamily x:Key="SpaceMonoSymbols">pack://application:,,,/Fonts/#Space Mono Symbols</FontFamily>
    </Application.Resources>
</Application>
